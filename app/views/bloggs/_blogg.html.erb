<tr>
  <td><%= blogg.title %></td>
  <td>Published <%= distance_of_time_in_words blogg.created_at, Time.now %> ago</td>
  <td><%= blogg.body %></td>
  <td><%= link_to blogg.status, toggle_status_blogg_path(blogg) %></td>
  <td><%= link_to 'Show', blogg %></td>
  <td><%= link_to 'Edit', edit_blogg_path(blogg) %></td>
  <td><%= link_to 'Destroy', blogg, method: :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>